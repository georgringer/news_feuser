{
  "name": "georgringer/news_feuser",
  "type": "typo3-cms-extension",
  "description": "Extend news by a relation to a FE User",
  "version": "1.0.2",
  "keywords": [
    "TYPO3",
    "extension",
    "news"
  ],
  "homepage": "http://ringer.it",
  "authors": [
    {
      "name": "Georg Ringer",
      "role": "Developer",
      "homepage": "http://ringer.it"
    }
  ],
  "license": [
    "GPL-2.0+"
  ],
  "require": {
    "typo3/cms-core": ">=6.2.10,<8.3",
    "georgringer/news": ">3.2.5"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.8.0"
  },
  "autoload": {
    "psr-4": {
      "GeorgRinger\\NewsFeuser\\": "Classes"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "GeorgRinger\\NewsFeuser\\Tests\\": "Tests",
      "TYPO3\\CMS\\Core\\Tests\\": ".Build/vendor/typo3/cms/typo3/sysext/core/Tests/",
      "TYPO3\\CMS\\Fluid\\Tests\\": ".Build/vendor/typo3/cms/typo3/sysext/fluid/Tests/"
    }
  },
  "replace": {
    "news-feuser": "self.version",
    "typo3-ter/news-feuser": "self.version"
  },
  "config": {
    "vendor-dir": ".Build/vendor",
    "bin-dir": ".Build/bin"
  },
  "scripts": {
    "post-autoload-dump": [
      "mkdir -p .Build/Web/typo3conf/ext/",
      "[ -L .Build/Web/typo3conf/ext/news_feuser ] || ln -snvf ../../../../. .Build/Web/typo3conf/ext/news_feuser"
    ]
  },
  "extra": {
    "typo3/cms": {
      "cms-package-dir": "{$vendor-dir}/typo3/cms",
      "web-dir": ".Build/Web"
    }
  }
}
